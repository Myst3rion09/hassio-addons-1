  {
  "name": "pdftohtml",
  "version": "0.1",
  "slug": "pdftohtml",
  "description": "PDF to HTML converter for Homeassitant",
  "url": "https://github.com/buktahula/hassio-addons/pdftohtml",
  "startup": "services",
  "boot": "auto",
  "arch": [
    "aarch64",
    "amd64",
    "armhf",
    "armv7",
    "i386"
  ],
   "options": {
     "config": [
       {
       "quelle": "/config/www/",
       "ziel": "/config/www/"
       }
      ]
     },
   "schema": {
     "config": [
       {
      "quelle": "str",
      "ziel": "str"
       }
      ]
     },
    "map": [
    "config:rw",
    "ssl:rw",
    "addons:rw",
    "share:rw",
    "backup:rw"
  ]
  }
